package Day4;
import java.util.Scanner;
public class Factors {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int i=num;
		int count=0;
		for(i=1;i<=num;i++) {
			if(num%i==0) {
				count++;
					
		    }
		}
		System.out.println(count);	
	}

}
